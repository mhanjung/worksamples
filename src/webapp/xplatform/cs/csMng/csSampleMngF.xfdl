/**********************************************************************************
 ★ description
    Add or subtract months
 ★ parameter 
    1. sDate : date ( example : "20121122" )
    2. nOffset : Add or subtract month ( example : 1 or -1 )
 ★ return 
    - success = yyyyMMdd type ( example : "20121202" or "20121022" )
    - failure = ""
**********************************************************************************/
function AddMonth(sDate, nOffset)
{
	if( IsNull(sDate) || IsNull(nOffset) )		return "";
	
    var nYear = parseInt(sDate.substr(0, 4));
    var nMonth = parseInt(sDate.substr(4, 2))+nOffset;
    var nDate = parseInt(sDate.substr(6, 2));
	var nLastDate, sRet;

	sRet = MakeDate(nYear, nMonth, nDate);	
	return sRet;
}

/*******************************************************************************
 ★ description
    Get today's date from this computers.
 ★ parameter 
 ★ return 
    - success = yyyyMMdd type ( example : "20121122" )
    - failure = ""
******************************************************************************/ 
function Today()
{
	var sToday = "";
	var objDate = new Date();
	var sToday  = objDate.getFullYear().toString();
	sToday += Right("0" + (objDate.getMonth() + 1), 2);
	sToday += Right("0" + objDate.getDate(), 2);

	return sToday;
}	

/*******************************************************************************
 ★ description
    Number to Date
 ★ parameter 
    1. nYear : year ( example : "2012" )
    2. nMonth : month ( example : "11" )
    3. nDate : date ( example : "22" )
 ★ return 
    - success = yyyyMMdd type ( example : "20121122" )
    - failure = ""
******************************************************************************/ 
function MakeDate(nYear, nMonth, nDate)
{
	if( IsNull(nYear) || IsNull(nMonth) || IsNull(nDate) )	return "";
	
    var objDate = new Date(nYear, nMonth-1, nDate);

	var sYear   = objDate.getFullYear().toString();
	var sMonth  = Right("0" + (objDate.getMonth() + 1), 2);
	var sDate   = Right("0" + objDate.getDate(), 2);

	return sYear + sMonth + sDate;
}

/*******************************************************************************
 ★ description
    Get the last day of month
 ★ parameter 
    1. sDate : yyyyMMdd type ( example : "20121122" )
 ★ return 
    - success = date ( example : "30" )
    - failure = ""
******************************************************************************/ 
function LastDateNum(sDate)
{
    var nMonth, nLastDate;

	if( IsNull(sDate) )		return -1;
	
	nMonth = parseInt(sDate.substr(4,2), 10);
    if( nMonth == 1 || nMonth == 3 || nMonth == 5 || nMonth == 7  || nMonth == 8 || nMonth == 10 || nMonth == 12 )
		nLastDate = 31;
    else if( nMonth == 2 )  
    {
        if( IsLeapYear(sDate) == true )
			nLastDate = 29;
        else
			nLastDate = 28;
    } 
    else 
		nLastDate = 30;
		
	return nLastDate;
}

/*******************************************************************************
 ★ description
    Return substring from the right.
 ★ Parameter 
    1. sOrg   : Original String ( example : "aaBBbbcc" )
    2. nSize  : size ( example : 2 )
 ★ return 
    - success = substring from the right. ( example : "cc" )
    - failure = ""
 ******************************************************************************/
function Right(sOrg, nSize)
{
	if( IsNull(sOrg) || IsNull(nSize) )		return "";
	
	if( sOrg.length < nSize )
		return sOrg;
	else
		return sOrg.substr(sOrg.length-nSize, nSize);
}

/*******************************************************************************
 ★ description
    Null check
 ★ Parameter 
    1. sValue : value ( example : null or undefined or "" or "abc" )
 ★ return 
    - If sValue is undefined, null, NaN, "", Array.length = 0, then return true
    - Otherwise = false
******************************************************************************/
function IsNull(sValue)
{
	if( new String(sValue).valueOf() == "undefined") 
		return true;
	if( sValue == null )
		return true;
	if( ("x"+sValue == "xNaN") && ( new String(sValue.length).valueOf() == "undefined" ) )
		return true;
	if( sValue.length == 0 )
		return true;
	return false;
}
