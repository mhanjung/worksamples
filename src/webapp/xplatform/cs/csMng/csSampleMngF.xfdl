{
	msk_srchRcptIdttNo.value = "";
	msk_srchRcptIdttNo.mask = "######-#######";
}


// GridSample onheaddblclick
function GridSample_onheaddblclick(obj:Grid, e:GridClickEventInfo)
{
	// sorting
    gfnGridSort(obj,e);
}

// member key setting
function gfnSetCstmNo(inCstmNo)
{
	var temp = "";
	var zeroCnt = 9 - inCstmNo.length;
	
	for(var i=0 ; i<zeroCnt ; i++){
		temp += "0";
	}
	var outCstmNo = temp + inCstmNo;
	
	return outCstmNo;
}

// Reset button onclick
function btn_reset_onclick(obj:Button,  e:ClickEventInfo)
{
	edt_srchGuCd.value = "";
	edt_srchGuNm.value = "";
	edt_srchAdstrdCdFrom.value = "";
	edt_srchAdstrdNmFrom.value = "";
	edt_srchAdstrdCdTo.value = "";
	edt_srchAdstrdNmTo.value = "";
	edt_srchMkey.value = "";
	edt_srchHhldNm.value = "";
	msk_srchHhldIdttNo.value = "";
	cbo_srchStts.value = "";
	edt_srchRcptNm.value = "";
	msk_srchRcptIdttNo.value = "";
	cal_srchAprpFrom.value = AddMonth( Today(), -1 );
	cal_srchAprpTo.value = Today();
	
	if( cbo_srchOff.enable == true ){
		cbo_srchOff.index = 0;
	}
	
	cbo_srchStts.index = 0;
}

// Search button onclick
function btn_Search_onclick(obj:Button,  e:ClickEventInfo)
{
	if ( cf_IsEssential(this, edt_srchGuCd.value, "edt_srchGuCd", "Region Code") ) {
		return;
	}
	else if ( cf_IsEssential(this, edt_srchAdstrdCdFrom.value, "edt_srchAdstrdCdFrom", "Area Code") ) {
		return;
	}
	else if ( cf_IsEssential(this, edt_srchAdstrdCdTo.value, "edt_srchAdstrdCdTo", "Area Code") ) {
		return;
	}
	else{
		search_sampleList();
	}
}

//search_sampleList
function search_sampleList(obj:Button,  e:ClickEventInfo)
{
	var oficeCd = "";
	var guCd = "";
	var adstrdCdFrom = "";
	var adstrdCdTo = "";
	var mkey = "";
	var hhldNm = "";
	var hhldIdttNo = "";
	var rcptNm = "";
	var rcptIdttNo = "";
	var rcptSttsCd = "";
	var rcptAprpDeFrom = "";
	var rcptAprpDeTo = "";
	
	if(cbo_srchOff.value != null){
		oficeCd = "oficeCd="+cbo_srchOff.value;
	}
	if(edt_srchGuCd.value != null){
		guCd = "guCd="+edt_srchGuCd.value;
	}
	if(edt_srchAdstrdCdFrom.value != null && edt_srchAdstrdCdTo.value != null){
		adstrdCdFrom = "adstrdCdFrom="+edt_srchAdstrdCdFrom.value;
		adstrdCdTo = "adstrdCdTo="+edt_srchAdstrdCdTo.value;
	}
	if(edt_srchMkey.value != null){
		if(edt_srchMkey.value != "" ){
			var realMkey = gfnSetCstmNo(edt_srchMkey.value);
			edt_srchMkey.value = realMkey;
			
			mkey = "mkey=" + realMkey;
		}
	}
	if(edt_srchHhldNm.value != null){
		if(edt_srchHhldNm.value != "" ){
			hhldNm = "hhldNm=" + edt_srchHhldNm.value;
		}
	}
	if(msk_srchHhldIdttNo.value != null){
		if(msk_srchHhldIdttNo.value != "" ){
			hhldIdttNo = "hhldIdttNo=" + msk_srchHhldIdttNo.value;
		}
	}
	if(edt_srchRcptNm.value != null){
		if(edt_srchRcptNm.value != "" ){
			rcptNm = "rcptNm=" + edt_srchRcptNm.value;
		}
	}
	if(msk_srchRcptIdttNo.value != null){
		if(msk_srchRcptIdttNo.value != "" ){
			rcptIdttNo = "rcptIdttNo=" + msk_srchRcptIdttNo.value;
		}
	}
	if(cal_srchAprpFrom.value != null){
		if(cal_srchAprpFrom.value != "" ){
			rcptAprpDeFrom = "rcptAprpDeFrom=" + cal_srchAprpFrom.value;
		}
	}
	if(cal_srchAprpTo.value != null){
		if(cal_srchAprpTo.value != "" ){
			rcptAprpDeTo = "rcptAprpDeTo=" + cal_srchAprpTo.value;
		}
	}
	if(cbo_srchStts.value != null){
		if(cbo_srchStts.value != "" ){
			rcptSttsCd = "rcptSttsCd=" + cbo_srchStts.value;
		}
	}
	
	arisuTransaction( this, "searchList", "svc::/cs/csMng/searchSample.do", "", "ds_sample=ds_output_01", oficeCd+" "+guCd+" "+adstrdCdFrom+" "+adstrdCdTo+" "+mkey+" "+hhldNm+" "+hhldIdttNo+" "+rcptNm+" "+rcptIdttNo+" "+rcptAprpDeFrom+" "+rcptAprpDeTo+" "+rcptSttsCd , "fnCallback");
}

// canrowposchange
function ds_sample_canrowposchange(obj:Dataset, e:DSRowPosChangeEventInfo)
{
	if( regAt_sampleRow.value == "Y" ){
		if(confirm("You are currently editing your information.\r\n Do you want to cancel the changes and go to the new row?", "warning", "icon=warning;lock=all")){
			return true;
		}
		else{
			return false;
		}
	}
}

// onrowposchanged
function ds_sample_onrowposchanged(obj:Dataset, e:DSRowPosChangeEventInfo)
{
	ds_sampleRow.clearData();
    var rowAdd = ds_sampleRow.addRow();
    ds_sampleRow.copyRow(rowAdd, ds_sample, ds_sample.rowposition);
	regAt_sampleRow.value = "N";
	fnRowEditDisable();
	
	if( ds_sampleRow.getColumn( 0, "rcptSttsCd") == "01"){
		sta_regDt.visible = true;
		cal_regDt.visible = true;
		sta_qualfFrfDe.visible = false;
		cal_qualfFrfDe.visible = false;
		sta_mvtDe.visible = false;
		cal_mvtDe.visible = false;
	}
	if( ds_sampleRow.getColumn( 0, "rcptSttsCd") == "02"){
		sta_regDt.visible = false;
		cal_regDt.visible = false;
		sta_qualfFrfDe.visible = true;
		cal_qualfFrfDe.visible = true;
		sta_mvtDe.visible = false;
		cal_mvtDe.visible = false;
	}
	if( ds_sampleRow.getColumn( 0, "rcptSttsCd") == "03"){
		sta_regDt.visible = false;
		cal_regDt.visible = false;
		sta_qualfFrfDe.visible = false;
		cal_qualfFrfDe.visible = false;
		sta_mvtDe.visible = true;
		cal_mvtDe.visible = true;
	}
}

