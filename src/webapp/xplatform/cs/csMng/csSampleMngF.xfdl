			else if (edt_srchAdstrdCdFrom.value == null || edt_srchAdstrdCdTo.value == null){
				alert("Please check your area code.");
			}
			else{
				search_sampleList();
			}
		}
		if(svcid == "updateSample"){
			alert("The information has been edited.");
			
			fnRowEditDisable();
			
			if(edt_srchGuCd.value == null){
				alert("Please enter your region code.");
			}
			else if (edt_srchAdstrdCdFrom.value == null || edt_srchAdstrdCdTo.value == null){
				alert("Please check your area code.");
			}
			else{
				search_sampleList();
			}
		}
		if(svcid == "updateCsInfo"){
			alert("The information has been edited.");
		}
	}
}

