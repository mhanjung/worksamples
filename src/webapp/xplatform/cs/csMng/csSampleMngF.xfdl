include "lib::common.xjs";


// voClass dataSet setting
function set_voClassDataSet()
{
	ds_voInfo.clearData();
	var rowAdd = ds_voInfo.addRow();
	ds_voInfo.setColumn(rowAdd, "voClass", "arisuinfo.cs.csMng.service.SampleMngVO");
}

// onload 
function csSampleMngF_onload(obj:Form, e:LoadEventInfo)
{
	gfnInIt(this.name);
	
	// office combo box setting
	gfnSetOff(obj, "cbo_srchOff", "", "1", "1");

	// if it's not from main office, then set the office code
	if(sso_officeId != "002"){
		var cboCnt = cbo_srchOff.getCount();	
		var matchIndex = 0;
		var matchAt = false;
	
		for(var i=0 ; i<cboCnt ; i++){
			cbo_srchOff.index = i;
			
			if(cbo_srchOff.value == sso_officeId){
				matchIndex = i;
				matchAt = true;
			}
		}
		
		// if it's not from main office, then disable this combo box
		if(matchAt){
			cbo_srchOff.index = matchIndex;
			cbo_srchOff.enable = false;
		// if it's from main office, then enable this combo box
		} else {
			cbo_srchOff.index = 0;
			cbo_srchOff.enable = true;
		}	
 	}
	
    // search status combo box setting
	gfnSetCmCd(obj, "cbo_srchStts", "CS046", "1", "1");
	// status radio setting
    gfnSetCmCd(obj, "rdo_stts", "CS046", "", "");
	
    // Starting date setting from
	cal_srchAprpFrom.value = AddMonth( Today(), -1 );
	// Starting date setting to
    cal_srchAprpTo.value = Today();
	
	// focus setting to guCd editbox
    var Focus = edt_srchGuCd.setFocus();
}

// Office combo box onitemchanged
function cbo_srchOff_onitemchanged(obj:Combo, e:ItemChangeEventInfo)
{
	edt_srchGuCd.value = "";
	edt_srchGuNm.value = "";
	edt_srchAdstrdCdFrom.value = "";
	edt_srchAdstrdNmFrom.value = "";
	edt_srchAdstrdCdTo.value = "";
	edt_srchAdstrdNmTo.value = "";
}

// Gu code edit box onitemchanged
function edt_srchGuCd_onkillfocus(obj:Edit, e:KillFocusEventInfo)
{
	var rtnObj = gfnGetGuName(obj, cbo_srchOff.value);
	edt_srchGuNm.value = rtnObj.guNm;
	
	edt_srchAdstrdCdFrom.value = "";
	edt_srchAdstrdNmFrom.value = "";
	edt_srchAdstrdCdTo.value = "";
	edt_srchAdstrdNmTo.value = "";
}

// Dong code(From) edit box onkillfocus
function edt_srchAdstrdCdFrom_onkillfocus(obj:Edit, e:KillFocusEventInfo)
{
	if(edt_srchGuCd.value == null){
		alert("Please enter your region code.");
		var Focus = edt_srchGuCd.setFocus();
	}
	else if(edt_srchGuCd.value == ""){
		alert("Please enter your region code.");
		var Focus01 = edt_srchGuCd.setFocus();
	}
	else{
		var rtnObj = gfnGetDongName(obj, edt_srchGuCd.value);
		edt_srchAdstrdNmFrom.value = rtnObj.dongNm;
		edt_srchAdstrdCdTo.value = edt_srchAdstrdCdFrom.value;
		edt_srchAdstrdNmTo.value = rtnObj.dongNm;
	}
}

// Dong code(To) edit box onkillfocus
function edt_srchAdstrdCdTo_onkillfocus(obj:Edit, e:KillFocusEventInfo)
{
	if(edt_srchGuCd.value == null){
		alert("Please enter your region code.");
		var Focus = edt_srchGuCd.setFocus();
	}
	else if(edt_srchGuCd.value == ""){
		alert("Please enter your region code.");
		var Focus01 = edt_srchGuCd.setFocus();
	}
	else{
		var rtnObj = gfnGetDongName(obj, edt_srchGuCd.value);
		edt_srchAdstrdNmTo.value = rtnObj.dongNm;
	}
}

// srchHhldIdttNo maskedit oneditclick
function msk_srchHhldIdttNo_oneditclick(obj:MaskEdit,  e:EditClickEventInfo)
{
	msk_srchHhldIdttNo.value = "";
	msk_srchHhldIdttNo.mask = "######-#######";
}

// srchRcptIdttNo maskedit oneditclick
